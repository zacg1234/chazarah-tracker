{
  "expo": {
    "name": "chazarah-tracker",
    "slug": "chazarah-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AALogoSmall.png",
    "scheme": "chazarahtracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "image": "./assets/images/AALogoSmall.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/AALogoSmall.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "adaptiveIcon": {
            "image": "./assets/images/AALogoSmall.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e58f4b34-20b3-4681-9ba8-6126866c0e9d"
      }
    }
  }
}
